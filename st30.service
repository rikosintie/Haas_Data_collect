[Unit]
Description=Logger for ST30
After=network.target

[Service]
User=mhubbard
WorkingDirectory=/home/mhubbard/Haas/Haas_Data_collect
ExecStart=/usr/bin/python3 /home/mhubbard/Haas/Haas_Data_collect/haas_logger2.py -a -t 192.168.0.141  --port 5053 --name st30
Type=idle

[Install]
WantedBy=multi-user.target