[Unit]
Description=Haas Python logger for VF5SS
After=network.target

[Service]
User=mhubbard
WorkingDirectory=/home/mhubbard/Haas/Haas_Data_collect
ExecStart=/usr/bin/python3 /home/mhubbard/Haas/Haas_Data_collect/haas_logger2.py -a -t 192.168.10.142  --port 5054 --name VF5SS
Type=idle

[Install]
WantedBy=multi-user.target
